<script>
phina.globalize();

phina.define('MainScene', {
  superClass: 'CanvasScene',
  
  init: function() {
    this.superInit();
    
    var label = Label('画面サイズを変えても\nキレイにフィットするよ♪').addChildTo(this);
    label.x = this.gridX.center();
    label.y = this.gridY.center();
    label.fontSize = 32;
  },
});

phina.main(function() {
  var app = GameApp({
    startLabel: 'main',
  });

  app.run();
});
</script>
